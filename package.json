{
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@cucumber/cucumber": "^8.11.1",
    "@playwright/test": "^1.30.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.13.0",
    "allure-commandline": "^2.21.0",
    "allure-playwright": "^2.0.0-beta.26",
    "axios": "^1.3.2",
    "bcrypt": "^5.1.0",
    "concurrently": "^7.6.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "fs-extra": "^11.1.0",
    "jsdom": "^21.1.0",
    "jsdom-global": "^3.0.2",
    "playwright": "^1.30.0",
    "playwright-chromium": "^1.30.0",
    "playwright-core": "^1.30.0",
    "playwright-firefox": "^1.30.0",
    "resolve": "^1.22.1",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "yarn": "^1.22.19"
  },
  "scripts": {
    "test": "yarn playwright test --grep @temp",
    "MS": "yarn playwright test --grep @API --workers=1 ",
    "code": "yarn playwright codegen",
    "mobile": "yarn playwright test Emulation.test.ts --project=Device",
    "startA": "yarn run:serial ",
    "startB": "yarn run:parallel ",
    "startS": "yarn clean:reports && yarn run:serial && yarn allure_results_rep && yarn allure_open_rep",
    "startP": "yarn clean:reports && yarn run:parallel && yarn allure_results_rep && yarn allure_open_rep",
    "run:serial": "playwright test --grep @LPL",
    "playwright_report": "npx playwright show-report html-report",
    "allure_results_rep": "npx allure generate allure-results -o allure-report --clean",
    "allure_open_rep": "npx allure open allure-report",
    "clean:reports": "rimraf allure-report && rimraf allure-results rimraf playwright-report"
  }
}